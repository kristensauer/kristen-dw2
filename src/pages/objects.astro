---
import Layout from "../layouts/Layout.astro";
import ProfileCard from "@components/ProfileCard.astro";

const kristen = {
  name: "Kristen",
  hometown: "Bayport",
  school: "SUNY New Paltz",
  age: 21,
};

const foods = [
    'Pizza',
    'Sushi',
    'Taco',
    'Burger',
    'Rice',
    'Wings',
];

console.log(foods[1]+" is my favorite food!");
---
<Layout title="Astro Starter">
  <ul>
    {foods.map(function (item) {
      return <li>{item}</li>
    })}
  </ul>
  <ProfileCard person={kristen} />
  </ul>
</Layout>